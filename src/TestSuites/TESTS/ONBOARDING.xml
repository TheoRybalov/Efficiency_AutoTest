<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ONBOARDING">


    <listeners>
        <listener class-name="Efficiency.TestListener.TestListener" />

    </listeners>

    <test name="About National Project (version for Phone)">
